dist: 
	@mkdir -p .tmp/fom && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) .tmp/fom/ && ( cd `dirname .tmp/fom` && $(TAR) fom.tar fom && $(GZIP) fom.tar ) && $(MOVE) `dirname .tmp/fom`/fom.tar.gz . && $(DEL_FILE) -r .tmp/fom

