####### Compile

main.o: main.cpp awari.h

awari.h: awari.ui 
	$(UIC) awari.ui -o awari.h

awari.cpp: awari.h awari.ui awari.ui.h 
	$(UIC) awari.ui -i awari.h -o awari.cpp

awari.o: awari.cpp awari.ui.h \
		awari.h

moc_awari.o: moc_awari.cpp  awari.h 

moc_awari.cpp: $(MOC) awari.h
	$(MOC) awari.h -o moc_awari.cpp

